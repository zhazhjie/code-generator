import request from '@/config/request'
<% if(retrieveFlag){ _%>
export function listPage(query) {
    return request({
        url: '<%=basePath%>/listPage',
        method: 'get',
        params: query
    })
}
<% } %>
<% if(createFlag){ _%>
export function addObj(obj) {
    return request({
        url: '<%=basePath%>/add',
        method: 'put',
        data: obj
    })
}
<% } %>
<% if(deleteFlag){ _%>
export function delObj(id) {
    return request({
        url: '<%=basePath%>/delete/' + id,
        method: 'delete'
    })
}
<% } %>
<% if(updateFlag){ _%>
export function updObj(obj) {
    return request({
        url: '<%=basePath%>/update',
        method: 'post',
        data: obj
    })
}
<% } %>

<% var mapper = mapperName.replace("%s", tableNameCapitalize) _%>
<% var entityType = entityName.replace("%s", tableNameCapitalize) _%>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="<%= basePackage %>.<%= mapper %>">
    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="<%= basePackage %>.<%= entityType %>">
    <%_ columns.forEach(column=>{ _%>
        <result column="<%= column.columnName %>" property="<%= column.property %>"/>
    <%_ }) _%>
    </resultMap>
</mapper>